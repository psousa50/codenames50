  "scripts": {
    "prebuild": "npm run clean",
    "postbuild": "npm test",
    "build": "tsc -d -p tsconfig.dist.json",
    "tsn": "ts-node",
    "tsc": "tsc --project tsconfig.json",
    "clean": "rm -rf lib",
    "test": "jest --testPathPattern=test/.+\\.spec\\.ts",
